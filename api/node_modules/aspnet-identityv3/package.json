{
  "name": "aspnet-identityv3",
  "version": "1.0.1",
  "description": "Typescript library for checking ASP.NET Identity V2 & V3 password",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Pavel Borisov",
    "email": "pavel.borisov@quercus.ru"
  },
  "scripts": {
    "test": "mocha -R mocha-multi --reporter-options spec=-,mocha-notifier-reporter=- dist/test",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha dist/test/*.js - - -R spec",
    "build": "tsc",
    "build-watch": "tsc -watch",
