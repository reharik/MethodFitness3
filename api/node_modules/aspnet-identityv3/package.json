  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.3.6",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-multi": "^1.0.1",
    "mocha-notifier-reporter": "^0.1.2",
    "testdouble": "^3.8.1",
    "typescript": "^2.9.2"
  }
